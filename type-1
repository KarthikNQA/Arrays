public static void main(String[] args) {
int[][]arr2d= new int [2][5];
		arr2d[0][0]=10;
		arr2d[0][1]=20;
		arr2d[0][2]=30;
		arr2d[0][3]=40;
		arr2d[0][4]=50;
		arr2d[1][0]=10;
		arr2d[1][1]=20;
		arr2d[1][2]=30;
		arr2d[1][3]=40;
		arr2d[1][4]=50;
		System.out.println(arr2d[0][0]+" "+arr2d[0][1]+" "+arr2d[0][2]+" "+arr2d[0][3]+" "+arr2d[0][4]+" "+arr2d[1][0]+" "+arr2d[1][1]+" "+arr2d[1][2]+" "+arr2d[1][3]+" "+arr2d[1][4]);
		}
    }
